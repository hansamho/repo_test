<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.infra.code.CodeDao">
<resultMap id="resultMapObj" type="com.example.demo.infra.code.CodeDto"></resultMap>


<select id="selectList" resultMap="resultMapObj">
       SELECT
            a.seq
            ,a.name
            ,a.cgCreate
            ,a.cgDelete
            ,(select aa.name from Code aa where 1=1 and aa.seq = a.code_seq) as codegroup_name
         FROM Code a
        WHERE 1=1
        ORDER BY a.Seq DESC
    </select>
	  
</mapper>